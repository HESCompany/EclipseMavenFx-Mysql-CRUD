<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="porto.java.PortoJava.UserInput">
   <children>
      <TableView fx:id="Table" editable="true" onMouseClicked="#Select">
        <columns>
          <TableColumn fx:id="C1" onEditCommit="#C1D" prefWidth="75.0" text="C1" />
          <TableColumn fx:id="C2" onEditCommit="#C2D" prefWidth="75.0" text="C2" />
            <TableColumn fx:id="C3" onEditCommit="#C3D" prefWidth="75.0" text="C3" />
            <TableColumn fx:id="Delete" onEditStart="#OnDelete" prefWidth="75.0" />
        </columns>
      </TableView>
      <HBox>
         <children>
            <TextField fx:id="C1F" promptText="C1" />
            <TextField fx:id="C2F" promptText="C2" />
            <TextField fx:id="C3F" promptText="C3" />
         </children>
      </HBox>
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="Create" mnemonicParsing="false" onAction="#OnCreate" text="Create" />
         </children>
      </HBox>
   </children>
</VBox>
